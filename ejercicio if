public class Main {

    public static void main(String[] args) {
        int numeroIf = 0;
        
        if(numeroIf > 0){
            System.out.println("Positivo");
        } else if (numeroIf < 0) {
            System.out.println("Negativo");
        }else {
            System.out.println(0);
        }
    }
}
