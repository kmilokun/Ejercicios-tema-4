public class Main {

    public static void main(String[] args) {
        String estacion = "Invierno";

        switch (estacion){
            case "Verano":
                System.out.println("Es "+estacion);
                break;
            case "Invierno":
                System.out.println("Es "+estacion);
                break;
            case "Primavera":
                System.out.println("Es "+estacion);
                break;
            case "Oto√±o":
                System.out.println("Es "+estacion);
                break;
            default:
                System.out.println(estacion+" no es una estacion");
        }

    }
}
